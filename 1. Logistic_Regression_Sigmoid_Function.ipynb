   "source": [
    "import numpy as np \n",
    "x = np.array([1,2,3])\n",
    "print(x)\n",
    "x_exp = np.exp(x)\n",
    "print(x_exp)\n",
    "y = 1\n",
    "print(np.exp(y))\n",
    "\n",
    "def sigmoid(z):\n",
    "\n",
    "    g = 1 / (1+np.exp(-z))\n",
    "    return g \n",
    "z_tmp = np.arange(-10,11)\n",
    "y = sigmoid(z_tmp)\n",
    "np.set_printoptions(precision = 3)\n",
    "print(np.c_[z_tmp , y ])\n"
   ]
